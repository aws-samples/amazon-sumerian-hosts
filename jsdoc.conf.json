{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "logoFile": "",
      "cleverLinks": false,
      "monospaceLinks": false,
      "dateFormat": "ddd MMM Do YYYY",
      "outputSourceFiles": false,
      "outputSourcePath": true,
      "systemName": "Amazon Sumerian Hosts",
      "footer": "",
      "copyright": "Â© 2020 Amazon Web Services, Inc or its affiliates. All rights reserved.",
      "navType": "horizontal",
      "linenums": true,
      "collapseSymbols": false,
      "inverseNav": true,
      "protocol": "html://",
      "methodHeadingReturns": false
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "docs_template",
    "destination": "docs/",
    "recurse": true,
    "readme": "./docs_template/readme.md"
  },
  "source": {
    "include": ["./packages/amazon-sumerian-hosts-core/src/core", "./packages/amazon-sumerian-hosts-three/src/three.js", "./packages/amazon-sumerian-hosts-babylon/src/Babylon.js"]
  }
}
